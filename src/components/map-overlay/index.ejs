<div class="map-overlay <%= locals.className %>">
	<div class="map-overlay--search">
		<div class="map-overlay--search-box">
			<div class="map-overlay--search">
				<input type="text" placeholder="Поиск теплоходов и круизных компаний">
				<%- include('../../icons/search.svg') %>
			</div>
		</div>

		<div class="map-overlay--search-results">
			<div class="map-overlay--search-overflow scroll-y">
				<div class="map-overlay--search-block map-overlay--search-companies ">
					<div class="map-overlay--search-title">
						<span>Круизные компании</span>
						<%- include('../../icons/expand.svg') %>
					</div>
					<div class="map-overlay--search-select" id="companyAllSelect">Выбрать всё</div>
					<div class="map-overlay--search-checks"></div>
				</div>
				<div class="map-overlay--search-block map-overlay--search-ships ">
					<div class="map-overlay--search-title">
						<span>Теплоходы</span>
						<%- include('../../icons/expand.svg') %>
					</div>
					<div class="map-overlay--search-select" id="shipsAllSelect">Выбрать всё</div>
					<div class="map-overlay--search-checks"></div>
				</div>
			</div>
		</div>
	</div>

	<div class="map-overlay--ship map-overlay--btn map-overlay--btn-primary-stroke">
		<%- include('../../icons/liner.svg') %>
	</div>
	<div class="map-overlay--anchor map-overlay--btn map-overlay--btn-primary-fill">
		<%- include('../../icons/stop.svg') %>
	</div>
	<div class="map-overlay--place map-overlay--btn map-overlay--btn-primary-stroke">
		<%- include('../../icons/showplace.svg') %>
	</div>
	<div class="map-overlay--menu map-overlay--btn map-overlay--toggle-btn map-overlay--btn-primary-stroke active">
		<div class="open"><%- include('../../icons/menu.svg') %></div>
		<div class="close"><%- include('../../icons/close.svg') %></div>
	</div>
	<div class="map-overlay--layers">
		<div class="map-overlay--overlays-box">
			<input type="checkbox" name="over" id="ship-layer-checkbox" class="map-overlay--layers-hidden map-overlay--layers-checkbox custom-radio" checked>
			<label for="ship-layer-checkbox" class="map-overlay--layers-hidden">Теплоходы</label>
			<input type="checkbox" name="over" id="stops-layer-checkbox" class="map-overlay--layers-hidden map-overlay--layers-checkbox custom-radio" checked>
			<label for="stops-layer-checkbox" class="map-overlay--layers-hidden">Стоянки</label>
			<input type="checkbox" name="over" id="sights-layer-checkbox" class="map-overlay--layers-hidden map-overlay--layers-checkbox custom-radio" checked>
			<label for="sights-layer-checkbox" class="map-overlay--layers-hidden">Места</label>
			<input type="checkbox" name="over" id="sunrises-layer-checkbox" class="map-overlay--layers-checkbox custom-radio" checked>
			<label for="sunrises-layer-checkbox">Восходы</label>
			<input type="checkbox" name="over" id="sunsets-layer-checkbox" class="map-overlay--layers-checkbox custom-radio" checked>
			<label for="sunsets-layer-checkbox">Закаты</label>
			<input type="checkbox" name="over" id="gateways-layer-checkbox" class="map-overlay--layers-checkbox custom-radio" checked>
			<label for="gateways-layer-checkbox">Шлюзы</label>
		</div>
	</div>

	<div class="map-overlay--time map-overlay--btn map-overlay--btn-primary-fill active">
		<%- include('../../icons/clock.svg') %>
	</div>
	<div class="map-overlay--line map-overlay--btn map-overlay--btn-primary-stroke">
		<%- include('../../icons/ruler.svg') %>
	</div>
	<div class="map-overlay--copy map-overlay--btn map-overlay--toggle-btn map-overlay--btn-primary-stroke active">
		<%- include('../../icons/layers.svg') %>
	</div>
	<div class="map-overlay--overlays">
		<div class="map-overlay--overlays-box">
			<input type="radio" name="over" id="over1" class="custom-radio" checked="">
			<label for="over1">OpenStreetMap</label>
			<input type="radio" name="over" id="over2" class="custom-radio">
			<label for="over2">Яндекс</label>
			<input type="radio" name="over" id="over3" class="custom-radio">
			<label for="over3">Яндекс. Спутник</label>
		</div>
	</div>

	<div class="map-overlay--range-dates">
		<div class="range--deco range--deco-left">-<div class="asteriks"
				tiptext="<div class='asteriks--box'>Подсказка для даты прибытия для посетителей</div>"
				data-title="<div class='asteriks--box'>Подсказка для даты прибытия для посетителей</div>">
				<%- include('../../icons/question.svg') %>
			</div>
		</div>
		<div class="range--deco range--deco-right">-</div>
		<input type="text" id="slider2" class="slider2" style="display: none;">
		<div class="rs-container sliding">
			<div class="rs-bg"></div>
			<div class="rs-pointer">
				<%- include('../../icons/liner.svg') %>
					<div class="rs-tooltip">-</div>
			</div>
		</div>
	</div>

	<div class="map-overlay--zoom-block">
		<div class="map-overlay--zoom-block-button map-overlay--zoom-block-button-plus">
			<%- include('../../icons/plus-circle.svg') %>
		</div>
		<div class="map-overlay--zoom-block-button map-overlay--zoom-block-button-minus">
			<%- include('../../icons/minus-circle.svg') %>
		</div>
		<div class="map-overlay--zoom-block-button map-overlay--zoom-block-button-reset">
			<%- include('../../icons/my-location.svg') %>
		</div>
	</div>

</div>
